{
  "name": "glanum-stack-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev -c \"npm run dev:serve\"",
    "dev:serve": "binode --require ./mocks -- @remix-run/serve:remix-serve ./build/index.js",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "",
    "start": "remix-serve ./build/index.js",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve ./build/index.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {
    "semi": false
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@remix-run/css-bundle": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "bcryptjs": "^2.4.3",
    "domain-functions": "^2.5.1",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "remix-auth": "^3.6.0",
    "remix-forms": "^2.2.1",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@remix-run/dev": "*",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.44.9",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "binode": "^1.0.5",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "happy-dom": "^12.10.3",
    "msw": "^2.0.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
